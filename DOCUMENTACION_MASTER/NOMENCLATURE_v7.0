        standard=get_standard_from_utcs(parts['utcs']),
        process=f"{system} {parts['domain']}",
        assembly=system,
        procedure=f"{domain_desc} Procedures",
        structure=system,
        operation=domain_desc,
        phase=get_flight_phase(parts['domain']),
        supply_chain=f"{system} Supply Chain"
    )
    
    # Generate short title
    short_title = f"{parts.get('product', '')} {parts.get('subproduct', '')} {parts['subtype']} - {system} {parts['domain']}"
    
    # Generate description
    description = generate_description(parts, system, domain_desc)
    
    # Generate keywords
    keywords = generate_keywords(parts, system, domain_desc)
    
    return {
        'title': title,
        'short_title': short_title,
        'description': description,
        'keywords': keywords,
        'language': 'en-US',
        'translations': generate_required_translations(parts, title, description)
    }
