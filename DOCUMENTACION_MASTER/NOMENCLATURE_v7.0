    }
    
    # Domain descriptions
    domain_descriptions = {
        'CPX': 'Complex Systems',
        'TSG': 'Troubleshooting',
        'FLT': 'Flight',
        'QNT': 'Quantum',
        'SWD': 'Software',
        'HWD': 'Hardware',
        'MDS': 'Multi-Domain Systems',
        'NET': 'Network',
        'SEC': 'Security',
        'ENV': 'Environmental',
        'SAF': 'Safety',
