    }
    
    # Product full names
    product_names = {
        ('AMP', 'BWB', 'Q100'): 'AMPEL360 BWB Q100',
        ('AMP', 'BWB', 'Q250'): 'AMPEL360 BWB Q250',
        ('AMP', 'EVT', 'CITY'): 'AMPEL360 eVTOL City',
        ('GAI', 'UAV', 'SWRM'): 'GAIA UAV Swarm',
        ('QUA', 'QPU', 'CORE'): 'Quantum QPU Core'
    }
    
    # Generate components
    template_key = (parts['phase'], parts['subtype'])
    template = title_templates.get(template_key, "{product_full} {subtype_name} - {domain_desc}")
    
    product_key = (parts['line'], parts.get('product', ''), parts.get('subproduct', ''))
    product_full = product_names.get(product_key, f"{parts['line']} {parts.get('product', '')} {parts.get('subproduct', '')}")
    
    system = extract_system_from_utcs(parts['utcs'])
    domain_desc = domain_descriptions.get(parts['domain'], parts['domain'])
    
    # Generate title
    title = template.format(
        product_full=product_full,
        system=system,
        domain_desc=domain_desc,
        component=f"{system} {parts['domain']}",
        module=f"{parts['domain']} Module",
